<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1080.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="400.0" prefWidth="100.0">
      <children>
          <TextField fx:id="username" promptText="username">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="140.0" />
            </VBox.margin>
         </TextField>
          <PasswordField fx:id="password" promptText="password">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
         </PasswordField>
                <Label fx:id="errorLabel" />
          <Button onAction="#logIn" prefHeight="31.0" prefWidth="704.0" text="Login">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
         </Button>
      </children>
      <VBox.margin>
         <Insets bottom="100.0" left="200.0" right="200.0" top="100.0" />
      </VBox.margin>
   </VBox>
</VBox>
